<h1>Create an Article</h1>

<%= form_for @article do |f| %>

    <P>
        <%= f.label :title %><br/>
        <%= f.text_field :title %>
    </p>
    <P>
        <%= f.label :description %><br/>
        <%= f.text_area :description %>
    </p>
    <p>
        <%= f.submit %>
    </p>
    
<% end %>